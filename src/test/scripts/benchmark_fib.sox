print "Starting Fibonacci Benchmark";

fn fib(n) {
  if (n < 2) return n;
  return fib(n - 2) + fib(n - 1);
}

var n = 20;
print "Calculating fib(" + n + ")...";

var start = clock();
var result = fib(n);
var duration = clock() - start;

print "Result: ";
print result;
// print "Time taken (seconds): ";
// print duration;
print "Done";
