// simple_module.sox - Minimal module for basic testing

var message = "Hello from simple_module!"
var value = 42

fn greet(name) {
    print("Hello, " + name + "!")
}

fn add(a, b) {
    return a + b
}

// Export - using table literal syntax
return {
    message: message,
    value: value,
    greet: greet,
    add: add
}
