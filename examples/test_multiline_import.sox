// test_multiline_import.sox - Test multi-line import syntax (Go-style)

print("=== Testing Multi-Line Import Syntax ===")
print("")

// Multi-line import with implicit names
print("1. Multi-line import with implicit names:")
print("   import (")
print("       \"simple_module\"")
print("       \"modules/math\"")
print("       \"modules/string_utils\"")
print("   )")
print("")

import (
    "simple_module"
    "modules/math"
    "modules/string_utils"
)

print("  simple_module.value = ")
print(simple_module.value)

print("  math.e = ")
print(math.e)

print("  string_utils.greeting = ")
print(string_utils.greeting)

print("")
print("2. Calling functions from all three modules:")

print("  simple_module.add(10, 20) = ")
print(simple_module.add(10, 20))

print("  math.abs(-42) = ")
print(math.abs(-42))

print("  string_utils.upper(\"hello\") = ")
print(string_utils.upper("hello"))

print("")
print("=== Multi-Line Import Test Complete ===")
